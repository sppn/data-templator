{
    "config_file": "Templator",
    "datasets": [
        {
            "name": "MyTable",
            "source": {
                "database": "SourceDB",
                "schema": "SourceSchema",
                "tablename": "SourceTableName",                
                "primary_key": [ { "name": "PersonID" }],
                "columns": [{ "name": "OrderID" },
                    { "name": "ProductName" },
                    { "name": "Quantity" }],
                "date_columns": [ {"name": "Date_Created" }]
            },
            "templates": [
                {
                    "name": "R2M_DATALAKESTOREGEN12DATABRICKS_DL4",
                    "generate": [
                        {
                            "language": "IPYNB",
                            "target": {
                                "database": "TargetDB",
                                "table": "{{source.database}}_{{ source.tablename}}",
                                "default_date_format": "{{ global.date_format }}"
                            },
                            "properties": {
                                "restore_notebook_path": "",
                                "reconcilliation_notebook_path": "SourceDB",
                                "backup_notebook_path": "blah",
                                "backup_folder": "s",
                                "logic_app_url": "https://prod-11.australiaeast.logic.azure.com:443/workflows/81aa8883a52945abba04289b9035b92c/triggers/manual/paths/invoke?api-version=2016-10-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=ig4jJ1uCudaaltTY37EBY7tF3HTgRdqvM1Zd9V4fOdo"
                            }
                        }
                    ]
                }
            ]
        }
    ],
    "global": {
        "restore_notebook_path": "yyyy-MM-dd",
        "reconcilliation_notebook_path": "SourceDB",
        "backup_notebook_path": "blah"
    }
}