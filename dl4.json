{
    "config_file": "Templator",
    "datasets": [
        {
            "name": "MyTable",
            "source": {
                "database": "SourceDB",
                "schema": "SourceSchema",
                "tablename": "SourceTableName",    
                "raw_folder": "test",
                "primary_key": [ { "name": "PersonID" }],
                "columns": [{ "name": "PersonID" },
                            { "name": "FirstName" },
                            { "name": "LastName" }],
                "date_columns": [ {"name": "Date_Created" }]
            },
            "templates": [
                {
                    "name": "R2M_DATALAKESTOREGEN12DATABRICKS_DL4",
                    "generate": [
                        {
                            "language": "IPYNB",
                            "target": {
                                "database": "TargetDB",
                                "table": "{{source.database}}_{{ source.tablename}}",
                                "date_format": "yyyy-MM-dd"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}